{
  "manifest_version": 2,

  "name": "RDV (Reddit Deja Vu)",
  "description": "This extension indicates all unread comments on a Reddit article.",
  "version": "1.1",

  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": ["*://www.reddit.com/*"],
      "css": ["rdv.css"],
      "js": ["jquery-1.9.1.min.js", "underscore-1.5.0.js", "firebase.js", "rdv.js"]
    }
  ]
}